# 启动项目的时候安装jd_scripts项目
# 如果是手动安装，可以关闭, false表示关闭
INSTALL_SCRIPTS_ON_START=false

# 非开发时作用，方便做代理
# web的访问路径，如'/test',则通过'localhost:8000/test'访问
# 同样也是web产物最终在jd_scripts_server/app/public的生成路径
CONTEXT_PATH=jd

# 非开发时作用
# 项目启动的端口
port=7802

# 非开发时作用
# 用于通过ps 查找进程
# 如果要在一台机器上启动多个应用，需要同时修改端口和title
title=x-jd-scripts
