    "concurrently": "^6.0.0"
  },
  "engines": {
    "node": ">=10.22.1"
  },
  "ci": {
    "version": "10"
  },
  "dependencies": {
    "dotenv": "^8.2.0"
  },
  "config": {
    "unsafe-perm": true
  }
